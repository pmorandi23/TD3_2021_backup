   434                                  
   435                                  ; Recorrer bus PCI. Bochs solo soporta bus cero ya que no tiene "PCI to PCI bridge".
   436                                  ; Cada bus tiene hasta 32 dispositivos.
   437                                  ; Si el dispositivo es compuesto puede tener hasta 8 funciones.
   438                                  ; Formato puerto 0x0CF8:
   439                                  ; Bit 31 = Siempre a "1"
   440                                  ; Bits 30-24 = Reservado (siempre a "0")
   441                                  ; Bits 23-16 = Número de bus (0 en nuestra aplicación)
   442                                  ; Bits 15-11 = Número de dispositivo (0-31)
   443                                  ; Bits 10-8 = Número de función (0-7)
   444                                  ; Bits 7-2 = Número de registro
   445                                  ; Bits 1-0: Siempre cero.
   446                                  
   447                                  recorrer_bus_pci:
   448 000010FC 66BD00400000            	mov 	ebp, 0x4000     	; Primera dirección de entrada/salida para PCI.
   449 00001102 66BF000000C0            	mov 	edi, 0xC0000000 	; Primera dirección a asignar a dispositivos PCI.
   450 00001108 66BE00000080            	mov 	esi, 0x80000000 	; Bus 0, dispositivo 0, función 0.
   451                                  
   452                                  ciclo_recorrer_bus_pci:
   453 0000110E 6681FE00000180          	cmp 	esi, 0x80010000 	; Verificar si se recorrió todo el bus 0.
   454 00001115 0F843001                	jz 		fin_recorrido_pci
   455                                  								; Leer registro 0: Bits 31-16: device ID,
   456                                  leer_registro_pci 0        		; Bits 15-0: vendor ID.
   456 00001119 66B800000000        <1>  mov eax, %1
   456 0000111F 6609F0              <1>  or eax, esi
   456 00001122 BAF80C              <1>  mov dx, 0x0CF8
   456 00001125 66EF                <1>  out dx, eax
   456 00001127 BAFC0C              <1>  mov dx, 0x0CFC
   456 0000112A 66ED                <1>  in eax, dx
   457 0000112C 83F8FF                  	cmp 	ax, 0xFFFF      	; Vendor ID 0xFFFF no existe.
   458 0000112F 7509                    	jnz 	dispositivo_hallado
   459 00001131 6681C600080000          	add 	esi,0x0800     		; Ir al siguiente dispositivo
   460 00001138 EBD4                    	jmp 	ciclo_recorrer_bus_pci
   461                                  
   462                                  dispositivo_hallado:			; Leer registro 0x0C: Bit 23: multiple function bit
   463                                  	leer_registro_pci 0x0C
   463 0000113A 66B80C000000        <1>  mov eax, %1
   463 00001140 6609F0              <1>  or eax, esi
   463 00001143 BAF80C              <1>  mov dx, 0x0CF8
   463 00001146 66EF                <1>  out dx, eax
   463 00001148 BAFC0C              <1>  mov dx, 0x0CFC
   463 0000114B 66ED                <1>  in eax, dx
   464 0000114D B508                    	mov 	ch, 8       	    ; Cantidad de funciones a procesar.
   465 0000114F 660FBAE017              	bt 		eax, 23
   466 00001154 7202                    	jc 		procesar_funcion
   467 00001156 B501                    	mov 	ch, 1
   468                                  	
   469                                  procesar_funcion:				; Leer registro 0: Bits 31-16: device ID,
   470                                  	leer_registro_pci 0      	; Bits 15-0: vendor ID.
   470 00001158 66B800000000        <1>  mov eax, %1
   470 0000115E 6609F0              <1>  or eax, esi
   470 00001161 BAF80C              <1>  mov dx, 0x0CF8
   470 00001164 66EF                <1>  out dx, eax
   470 00001166 BAFC0C              <1>  mov dx, 0x0CFC
   470 00001169 66ED                <1>  in eax, dx
   471 0000116B 83F8FF                  	cmp  	ax, 0xFFFF    		; Vendor ID 0xFFFF no existe.
   472 0000116E 7523                    	jnz 	funcion_hallada   
   473                                  
   474                                  ir_a_siguiente_funcion:
   475 00001170 6681C600010000          	add 	esi, 0x0100    		; Ir a la siguiente función
   476 00001177 FECD                    	dec 	ch
   477 00001179 75DD                    	jnz 	procesar_funcion
   478 0000117B 6681EE00010000          	sub 	esi, 0x0100     	; Ir al siguiente dispositivo
   479 00001182 6681CE00070000          	or		esi, 0x0700
   480 00001189 6681C600010000          	add 	esi, 0x0100
   481 00001190 E97BFF                  	jmp 	ciclo_recorrer_bus_pci
   482                                  
