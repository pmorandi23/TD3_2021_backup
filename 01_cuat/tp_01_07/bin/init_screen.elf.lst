   487 000011B7 BAF80C              <1>  mov dx, 0x0CF8
   487 000011BA 66EF                <1>  out dx, eax
   487 000011BC BAFC0C              <1>  mov dx, 0x0CFC
   487 000011BF 66ED                <1>  in eax, dx
   488 000011C1 B110                    	mov 	cl, 0x10         	; Apuntar al primer Base Address Register (BAR)
