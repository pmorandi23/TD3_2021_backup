     1                                  ;##########################################################
     2                                  ;				init_screen.ASM
     3                                  ;##########################################################
     4                                  SECTION .vga_init
     5                                  
     6                                  %macro 		leer_registro_pci 1 				; 1 parámetro
     7                                  	mov 	eax, %1
     8                                  	or 		eax, esi
     9                                  	mov		dx, 0x0CF8
    10                                  	out 	dx, eax    							; Escribir palabra de configuración.
    11                                  	mov 	dx, 0x0CFC
    12                                  	in 		eax, dx   							; Verificar si existe el dispositivo.
    13                                  %endmacro
