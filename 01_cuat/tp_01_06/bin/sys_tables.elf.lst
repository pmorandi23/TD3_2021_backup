    61 00000015 92                          db 10010010b    ;Atributos:
    62                                                          ;P=1 Presente en el segmento
    63                                                          ;DPL=00 Privilegio nivel 0 - Kernel
    64                                                          ;S=1 Descriptor de Codigo/Datos                        
    65                                                          ;D/C=0 Segmento de Datos 
    66                                                          ;ED=0 Segmento de datos comun
    67                                                          ;W=1 Escribible
    68                                                          ;A=0 por defecto No Accedido
    69 00000016 CF                          db 11001111b    ;Limit
    70                                                      ;G
    71                                                      ;D/B
    72                                                      ;L
    73                                                      ;AVL
    74 00000017 00                          db 0x00         ;Base
