    54                                                          ;L=0 No 64 bits nativo
    55                                                          ;AVL=0 No utilizado
    56 0000000F 00                          db 0x00         ;Base
    57                                  DS_SEL_32      equ $-GDT_32       ;0X20
    58 00000010 FFFF                        dw 0xFFFF       ;Limit 15-0
    59 00000012 0000                        dw 0x0000       ;Base 15-0
