    39 00000008 FFFF                        dw 0xFFFF       ;Limit 15-0
    40 0000000A 0000                        dw 0x0000       ;Base 15-0
    41 0000000C 00                          db 0x00         ;Base 23-16
    42 0000000D 99                          db 10011001b    ;Atributos:                   
    43                                                          ;P=1 Presente en el segmento
    44                                                          ;DPL=00 Privilegio nivel 0 - Kernel
    45                                                          ;S=1 Descriptor de Codigo/Datos        
    46                                                          ;D/C=1 Segmento de Codigo 
    47                                                          ;C=0 No puede ser invocado
    48                                                          ;R=0 No legible
    49                                                          ;A=1 por defecto Accedido
